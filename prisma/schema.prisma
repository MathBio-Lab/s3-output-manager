generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

enum UserType {
  admin
  client
  team
}

model User {
  id        Int       @id @default(autoincrement())
  username  String    @unique
  password  String
  prefix    String?
  metadata  Json?
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
  deletedAt DateTime?
  type      UserType
}
